{
  "name": "parrot-layout",
  "version": "1.10.7",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf lib #",
    "compile": "webpack --progress --colors --config ./webpack.config.js",
    "copy-d-ts": "mkdir -p lib/ && cp lib/index.d.ts lib/bundle.d.ts",
    "copy-assets": "mkdir -p lib/ && cp favicon.ico lib/",
    "copy-config": "mkdir -p lib/ && cp package.json lib/",
    "compile-test": "webpack --progress --colors --config ./test.config.js",
    "serve": "http-server ./build",
    "build": "npm run compile && npm run copy-d-ts && npm run copy-config",
    "clean-build": "npm run clean && npm run build",
    "test": "npm run clean-build && npm run compile-test && npm run serve",
    "deploy": "npm run clean-build && npm publish lib",
    "dev": "webpack-dev-server --progress --colors --inline --config ./hot.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sztukauniwersalna/parrot-layout.git"
  },
  "keywords": [
    "sztukauniwersalna",
    "blog",
    "react",
    "layout",
    "parrot"
  ],
  "author": "Maciej Cha≈Çapuk (maciej@chalapuk.pl)",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/sztukauniwersalna/parrot-layout/issues"
  },
  "homepage": "https://github.com/sztukauniwersalna/parrot-layout#readme",
  "devDependencies": {
